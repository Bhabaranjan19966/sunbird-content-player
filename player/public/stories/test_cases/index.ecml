<theme id="theme" startStage="textPluginBasic" ver="0.2">
    <manifest>
        <media id="previous" src="icon_previous.png" type="image" />
        <media id="next" src="icon_next.png" type="image" />
        <media id="icon_reload" src="icon_reload.png" type="image" />
        <media id="icon_home" src="icon_home.png" type="image" />
        <media id="bg" src="felt.png" type="image" />
        <media id="t-outer" src="tailor-outer.png" type="image" />
        <media id="t-inner" src="tailor-inner.png" type="image" />
        <media id="w-outer" src="wife-outer.png" type="image" /> 
        <media id="barber_img" src="barber.png" type="image" />
        <media id="carpenter_img" src="carpenter.png" type="image" />
        <media id="tailor_img" src="tailor.png" type="image" />
        <media id="wife_img" src="wife.png" type="image" />       
        <media id="grey" src="grey.png" type="image" />
        <media id="potatohighlight" src="potato.png" type="image" />
        <media id="rabbithighlight" src="rabbit.png" type="image" />
        <media id="w-inner" src="wife-inner.png" type="image" />
        <media id="b-outer" src="barber-outer.png" type="image" />
        <media id="b-inner" src="barber-inner.png" type="image" />
        <media src="sounds/splash.ogg" id="splash_audio" type="audiosprite">
            <data>
                <audioSprite id="cover_sound" startTime="0" duration="7131.428571428572"></audioSprite>
            </data>
        </media>
        <media src="sounds/scene19.mp3" id="scene19_audio" type="audiosprite">
            <data>
                <audioSprite id="scene19_sound" startTime="0" duration="12303.673469387755"></audioSprite>
            </data>
        </media>
        <media src="sounds/drag.mp3" id="drag_audio" type="audiosprite">
            <data>
                <audioSprite id="drag_audio" startTime="1" duration="20.00"></audioSprite>
            </data>
        </media>
    </manifest>
    
    <controller name="testData" type="data" id="testdata"/>
    <controller name="assessment" type="items" id="assessment"/>
    <controller name="worksheet" type="items" id="worksheet"/>
    <controller name="worksheet2" type="items" id="worksheet2"/>
    <controller name="worksheet3" type="items" id="worksheet3"/>
    <template id="mcq_template_1">
        <text model="item.title" x="9" y="7" w="86" h="4" font="Georgia" fontsize="42"/>
        <mcq multi_select="false" model="item" shadow="blue" blur="80" offsetX="50" >
            <options layout="table" x="20" y="15" w="70" h="85" cols="2" marginX="10" marginY="5" options="options">
                <events>
                    <event type="click">
                        <action type="command" command="play" asset="scene19_sound" loop="1" />
                    </event>
                </events>
            </options>
        </mcq>
    </template>
    <template id="mtf_template_2">
        <text model="item.title" x="9" y="7" w="86" h="4" font="Georgia" fontsize="42"/>
        <mtf model="item" force="true">
            <option x="20" y="15" w="30" h="25" snapX="42" snapY="9" option="lhs_options[0]"/>
            <option x="20" y="43" w="30" h="25" snapX="10.5" snapY="5" option="lhs_options[1]"/>
            <option x="20" y="71" w="30" h="25" snapX="27.5" snapY="8.5" option="lhs_options[2]"/>

            <option x="70" y="15" w="10" h="16" option="rhs_options[0]"/>
            <option x="70" y="43" w="10" h="19.5" option="rhs_options[1]"/>
            <option x="70" y="71" w="12" h="22" option="rhs_options[2]">
                <events>
                    <event type="mousedown">
                        <action type="command" command="play" asset="drag_audio" audio="true" loop="1" />
                    </event>
                    <event type="pressup">
                        <action type="command" command="stop" asset="drag_audio" audio="true" loop="1" />
                    </event>
                </events>
            </option>
        </mtf>
        <g x="9" y="17" w="20" h="20" id="hint" visible="false">
            <!-- <image asset="speech_bubble" x="0" y="0" w="100" h="100" /> -->
            <text x="10" y="20" w="80" h="80" font="Georgia" weight="bold" fontsize="150">
                Hint
            </text>
            <text x="10" y="40" w="80" h="80" font="Georgia" fontsize="120" model="item.hints[0].asset"/>
        </g>
    </template>

    <stage id="tempBaseStage" preload="true">
        <image asset="b-outer"/>
        <image asset="w-outer"/>
        <image asset="t-outer"/>
        <image asset="b-inner"/>
        <image asset="w-inner"/>
        <image asset="t-inner"/>
        <image asset="barber_img" />
        <image asset="carpenter_img" />
        <image asset="tailor_img" />
        <image asset="wife_img" />
        <audio asset="scene19_audio" />
    </stage>
	<stage id="baseStage" preload="true">
        <image asset="next" x="93" y="3" w="5" h="8.3" id="next" visible="false"/>
        <shape id="nextContainer" type="rect" x="90" y="1" w="10" h="15" visible="false" hitArea="true">
            <event type="click">
                <action type="command" command="transitionTo" asset="theme" param="next" effect="scroll" direction="left" ease="linear" duration="500" />
            </event>
        </shape>
        <image asset="previous" x="2" y="3" w="5" h="8.3" id="previous" visible="false"/>
        <shape id="previousContainer" type="rect" x="1" y="1" w="10" h="15" visible="false" hitArea="true">
            <event type="click">
                <action type="command" command="transitionTo" asset="theme" param="previous" effect="scroll" direction="right" ease="linear" duration="500" transitionType="previous"/>
            </event>
        </shape>
        <text param="heading" x="28" y="2" w="62" h="23" weight="bold" fontsize="50" color="#fff"/>
    </stage>

    <stage id="textPluginBasic" x="0" y="0" w="100" h="100" extends="baseStage">
    	<param name="next" value="textPluginWrap" />
    	<!-- <param name="previous" value="textPluginBasic" /> -->

    	<image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_textpg_align"/>

        <text x="5" y="10" h="10" w="25" fontsize="30" color="yellow">Default alignment - should be aligned as top-left, by default</text>
        <shape x="5" y="20" h="10" w="15" type="rect" stroke="white"/>
        <text x="5" y="20" h="10" w="15" fontsize="60" color="white">TopLeft</text>


        <text x="5" y="40" h="10" w="25" fontsize="30" color="yellow">Horiz alignments - should be aligned as displayed</text>
        <shape x="5" y="50" h="10" w="15" type="rect" stroke="white"/>
        <text x="5" y="50" h="10" w="15" fontsize="60" color="white" align="left">Left</text>
        
        <shape x="25" y="50" h="10" w="15" type="rect" stroke="white"/>
        <text x="25" y="50" h="10" w="15" fontsize="60" color="white" align="center">Center</text>
        
        <shape x="45" y="50" h="10" w="15" type="rect" stroke="white"/>
        <text x="45" y="50" h="10" w="15" fontsize="60" color="white" align="right">Right</text>


        <text x="5" y="70" h="10" w="25" fontsize="30" color="yellow">Vert alignments - should be aligned as displayed</text>
        <shape x="5" y="80" h="15" w="15" type="rect" stroke="white"/>
        <text x="5" y="80" h="15" w="15" fontsize="60" color="white" align="center" valign="top">Top</text>
        
        <shape x="25" y="80" h="15" w="15" type="rect" stroke="white"/>
        <text x="25" y="80" h="15" w="15" fontsize="60" color="white" align="center" valign="middle">Middle</text>
        
        <shape x="45" y="80" h="15" w="15" type="rect" stroke="white"/>
        <text x="45" y="80" h="15" w="15" fontsize="60" color="white" align="center" valign="bottom">Bottom</text>

        <text  x="65" y="55" h="10" w="30" fontsize="30" color="yellow">Both alignments - should be aligned as displayed</text>
        <shape x="65" y="65" h="30" w="30" type="rect" stroke="white"/>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="left" valign="top">TopLeft</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="center" valign="top">TopCenter</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="right" valign="top">TopRight</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="left" valign="middle">MiddleLeft</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="center" valign="middle">MiddleCenter</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="right" valign="middle">MiddleRight</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="left" valign="bottom">BottomLeft</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="center" valign="bottom">BottomCenter</text>
        <text  x="65" y="65" h="30" w="30" fontsize="30" color="white" align="right" valign="bottom">BottomRight</text>
    </stage> 

    <stage id="textPluginWrap" x="0" y="0" w="100" h="100" extends="baseStage">
    	<param name="next" value="mcqOptionsGlow" />
    	<param name="previous" value="textPluginBasic" />

    	<image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_textpg_wrap"/>

        <text x="10" y="10" h="10" w="20" fontsize="30" color="yellow">Default alignments - should be aligned to top-left</text>
        <text x="10" y="20" h="10" w="20" fontsize="40" color="white" stroke="white">This text is aligned to top-left by default.</text>

        <text x="10" y="40" h="10" w="20" fontsize="30" color="yellow">Horiz alignments - should be aligned as displayed</text>
        <text x="10" y="50" h="10" w="20" fontsize="40" color="white" stroke="white" align="left">This text is left aligned by setting.</text>
        <text x="30" y="50" h="10" w="20" fontsize="40" color="white" stroke="white" align="center">Use align="center" to align text in center</text>
        <text x="50" y="50" h="10" w="20" fontsize="40" color="white" stroke="white" align="right">Use align="right" to align text at right</text>

        <text x="10" y="70" h="10" w="20" fontsize="30" color="yellow">Vert alignments - should be aligned as displayed</text>
        <text x="10" y="80" h="15" w="20" fontsize="40" color="white" stroke="white" align="center" valign="top">Use valign="top" for vertical top alignment</text>
        <text x="30" y="80" h="15" w="20" fontsize="40" color="white" stroke="white" align="center" valign="middle">Use valign="middle" for vertical middle alignment</text>
        <text x="50" y="80" h="15" w="20" fontsize="40" color="white" stroke="white" align="center" valign="bottom">Use valign="bottom" for vertical bottom alignment</text>

        <text x="73" y="15" h="10" w="20" fontsize="30" color="yellow">LineHeight for Multiline Text</text>
        <text x="73" y="20" h="40" w="20" fontsize="30" color="white" stroke="white">Text with default line height. Wrap should not overlap</text>
        <text x="73" y="30" h="10" w="20" fontsize="30" color="white" lineHeight="1.5">Text with line height 1.5. Wrap should not overlap</text>
        <text x="73" y="40" h="10" w="20" fontsize="30" color="white" lineHeight="0.3">Text with line height 0.3. Wrap should overlap</text>
        <text x="73" y="50" h="10" w="20" fontsize="30" color="white" lineHeight="-1">Text with line height -1. Wrap should go on top</text>

        <text x="73" y="65" h="10" w="20" fontsize="30" color="yellow">Advanced Text Properties</text>
        <text x="73" y="70" h="20" w="20" fontsize="40" color="white" stroke="white" outline="2">Text With Outline</text>
        <text x="73" y="75" h="10" w="20" fontsize="40" color="white" shadow="red" blur="10" offsetY="5">Text with Shadow</text>
        <text x="73" y="80" h="10" w="20" fontsize="40" color="white" shadow="red" blur="10" offsetY="5" outline="2">Text with Outline and Shadow</text>
    </stage> 

    <stage id="mcqOptionsGlow" x="0" y="0" w="100" h="100" extends="baseStage">
        <param name="next" value="launchExternal" />
        <param name="previous" value="textPluginWrap" />

        <image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_mcq_glow"/>

        <text x="10" y="10" h="10" w="40" fontsize="30" color="yellow">TO-DO</text>


    </stage>

    <stage id="launchExternal" x="0" y="0" w="100" h="100" extends="baseStage">
        <param name="next" value="conditionalRendering1" />
        <param name="previous" value="mcqOptionsGlow" />

        <image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_external_launch"/>


        <text x="10" y="10" h="10" w="40" fontsize="30" color="yellow">External URL - should be launched when URL is clicked</text>
        <text x="10" y="15" w="45" h="5" font="Arial" fontsize="48" color="white">
            Go to ekstep.org
        </text>
        <shape type="rect" x="10" y="15" w="35" h="7" hitArea="true">
            <event type="click">
                <action type="command" command="external" href="http://www.ekstep.org/"/>
            </event>
        </shape>    

        <text x="10" y="40" h="10" w="40" fontsize="30" color="yellow">External app - should be launched when link is clicked</text>
        <text x="10" y="45" w="45" h="15" font="Arial" fontsize="48" color="white">
            Launch whatsapp
        </text>
        <shape type="rect" x="10" y="50" w="20" h="65">
            <event type="click">
                <action type="command" command="external" app="com.whatsapp"/>
            </event>
        </shape>

    </stage>
    <stage id="conditionalRendering1" x="0" y="0" w="100" h="100" extends="baseStage">
        <param name="next" value="conditionalRendering2" />
        <param name="previous" value="launchExternal" />

        <set param="param1" value="value1" scope="stage"/>
        <set param="param2" ev-value="stage.param1 + '_new'" scope="stage"/>

        <image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_cond_render_stage1"/>

        <text x="10" y="15" h="10" w="80" fontsize="40" color="yellow">Text plugin: expression evaluates to false - should not show text</text>
        <text x="70" y="15" h="10" w="80" fontsize="30" color="white" ev-if="stage.param2 == 'value_new'">Invalid expression. This expression shouldn't be seen.</text>
        <text x="10" y="25" h="10" w="80" fontsize="40" color="yellow">Text plugin: expression evaluates to true - should show text below.</text>
        <text x="70" y="25" h="10" w="80" fontsize="30" color="white" ev-if="stage.param2 == 'value1_new'">Got True. Showing the text.</text>

        <text x="10" y="35" h="10" w="80" fontsize="40" color="yellow">Image plugin: expression evaluates to false - should not show image</text>
        <image asset="icon_home" x="70" y="35" w="10" h="10" ev-if="stage.param2 == 'value_new'"/>
        <text x="10" y="45" h="10" w="80" fontsize="40" color="yellow">Image plugin: expression evaluates to true - should show image</text>
        <image asset="icon_home" x="70" y="45" w="10" h="10" ev-if="stage.param2 == 'value1_new'"/>

        <text x="10" y="55" h="10" w="80" fontsize="40" color="yellow">Shape plugin: expression evaluates to false - should not show shape</text>
        <shape type="rect" x="70" y="55" w="5" h="5" hitArea="true" stroke="red" ev-if="stage.param2 == 'value_new'"></shape>
        <text x="10" y="65" h="10" w="80" fontsize="40" color="yellow">Shape plugin: expression evaluates to true - should show shape</text>
        <shape type="rect" x="70" y="65" w="5" h="5" hitArea="true" stroke="white" ev-if="stage.param2 == 'value1_new'"></shape>

         <!-- Input Plugin tests -->
        <text x="10" y="75" h="10" w="80" fontsize="40" color="yellow">Input plugin: expression evaluates to false - should not show input field</text>
        <input type="number" class="inputText" x="70" y="75" w="7" h="7" id="text1" ev-if="stage.param2 == 'value_new'"/>
        <text x="10" y="85" h="10" w="80" fontsize="40" color="yellow">Input plugin: expression evaluates to true - should show input field</text>
        <input type="number" class="inputText" x="70" y="85" w="7" h="7" id="text1" ev-if="'a' != 'b'"/>

        <!--Audio Plugin -->        
        <!-- <text x="10" y="80" h="10" w="80" fontsize="40" color="yellow">Audio plugin: expression evaluates to false - should not play audio</text>
        <audio asset="splash_audio" ev-if="stage.param2 == 'value_new'"/>
        <text x="10" y="90" h="10" w="80" fontsize="40" color="yellow">Audio plugin: expression evaluates to true - should play audio</text>
        <audio asset="splash_audio" ev-if="stage.param2 == 'value1_new'"/> -->

                
    </stage>

    <stage id="conditionalRendering2" x="0" y="0" w="100" h="100" extends="baseStage">
        <param name="next" value="conditionalRendering3" />
        <param name="previous" value="conditionalRendering1" />

        <image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_cond_render_stage2"/>

        <set param="param1" value="value1" scope="stage"/>
        <set param="param2" ev-value="stage.param1 + '_new'" scope="stage"/>

       
        <!--Animation -->
        <text x="10" y="15" h="10" w="40" fontsize="40" color="yellow">Animation plugin: expression evaluates to true - object should be animated on click.</text>
        <image asset="icon_home" x="11.5" y="35" w="10" h="10" id="home">
            <event type="click">
                <action type="animation" ev-if="stage.param2 == 'value1_new'">
                    <tween id="homeWalking">
                        <to ease="linear" duration="500">
                            <![CDATA[{"x":20,"y":20}]]>
                        </to>
                        <to ease="quadOut" duration="2000">
                            <![CDATA[{"x":55,"y":0}]]>
                        </to>
                        <to ease="linear" duration="1">
                            <![CDATA[{"x":75,"y":0, "scaleX": -1}]]>
                        </to>
                        <to ease="linear" duration="2000">
                            <![CDATA[{"x":20,"y":35}]]>
                        </to>                        
                    </tween>
                </action>
            </event>
        </image>
        <text x="55" y="15" h="10" w="40" fontsize="40" color="yellow">Animation plugin: expression evaluates to false - object should not be animated on click.</text>
        <image asset="icon_home" x="55.5" y="35" w="10" h="10" id="home">
            <event type="click">
                <action type="animation" ev-if="stage.param2 == 'value_new'">
                    <tween id="homeWalking">
                        <to ease="linear" duration="500">
                            <![CDATA[{"x":20,"y":20}]]>
                        </to>
                        <to ease="quadOut" duration="2000">
                            <![CDATA[{"x":55,"y":0}]]>
                        </to>
                        <to ease="linear" duration="1">
                            <![CDATA[{"x":75,"y":0, "scaleX": -1}]]>
                        </to>
                        <to ease="linear" duration="2000">
                            <![CDATA[{"x":40,"y":35}]]>
                        </to>
                        
                    </tween>
                </action>
            </event>
        </image>

        <text x="10" y="75" h="10" w="40" fontsize="40" color="yellow">Tween / Sprite?: expression evaluates to false - - TO-DO.</text>

        <text x="55" y="75" h="10" w="40" fontsize="40" color="yellow">Tween / Sprite?: expression evaluates to true -  -- TO-DO</text>

    </stage>

   <stage id="conditionalRendering3" x="0" y="0" w="100" h="100" extends="baseStage">
        <param name="next" value="conditionalRendering4" />
        <param name="previous" value="conditionalRendering2" />

        <image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_cond_render_stage3"/>

        <set param="param1" value="value1" scope="stage"/>
        <set param="param2" ev-value="stage.param1 + '_new'" scope="stage"/>

        <text x="10" y="15" h="10" w="40" fontsize="40" color="yellow">Event: expression evaluates to false - object should not be clickable.</text>
        <image asset="icon_home" x="15.5" y="35" w="10" h="10" id="home">
            <event type="click" ev-if="stage.param2 == 'value_new'">
            </event>
        </image>

        <text x="55" y="15" h="10" w="40" fontsize="40" color="yellow">Event: expression evaluates to true - object should be clickable.</text>
        <image asset="icon_home" x="65.5" y="35" w="10" h="10" id="home">
            <event type="click" ev-if="stage.param2 == 'value1_new'">
            </event>
        </image>

        <text x="10" y="45" h="10" w="40" fontsize="40" color="yellow">Action: expression evaluates to false - action should not work - TO-DO.</text>

        <text x="55" y="45" h="10" w="40" fontsize="40" color="yellow">Action: expression evaluates to true - action should work. -- TO-DO</text>     
        
        <text x="10" y="75" h="10" w="40" fontsize="40" color="yellow">Controller: expression evaluates to false - - TO-DO.</text>

        <text x="55" y="75" h="10" w="40" fontsize="40" color="yellow">Controller: expression evaluates to true -  -- TO-DO</text>


    </stage>

    <stage id="conditionalRendering4" x="0" y="0" w="100" h="100" extends="baseStage">
        <param name="next" value="conditionalRendering5" />
        <param name="previous" value="conditionalRendering3" />

        <image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_cond_render_stage4"/>

        <set param="param1" value="value1" scope="stage"/>
        <set param="param2" ev-value="stage.param1 + '_new'" scope="stage"/>

        <text x="10" y="15" h="10" w="40" fontsize="40" color="yellow">MCQ: expression evaluates to false - MCQ shouldn't be displayed(???) - TO-DO.</text>

        <text x="55" y="15" h="10" w="40" fontsize="40" color="yellow">MCQ: expression evaluates to true - MCQ should be displayed(???) - TO-DO.</text>

        <text x="10" y="30" h="10" w="40" fontsize="40" color="yellow">MTF: expression evaluates to false - MTF shouldn't be displayed(???) - TO-DO.</text>

        <text x="55" y="30" h="10" w="40" fontsize="40" color="yellow">MTF: expression evaluates to true - MTF should be displayed(???) - TO-DO.</text>

        <text x="10" y="45" h="10" w="40" fontsize="40" color="yellow">option: expression evaluates to false - option shouldn't be displayed(???) - TO-DO.</text>

        <text x="55" y="45" h="10" w="40" fontsize="40" color="yellow">option: expression evaluates to true - option should be displayed(???) - TO-DO.</text>

        <text x="10" y="60" h="10" w="40" fontsize="40" color="yellow">options: expression evaluates to false - options shouldn't be displayed(???) - TO-DO.</text>

        <text x="55" y="60" h="10" w="40" fontsize="40" color="yellow">options: expression evaluates to true - options should be displayed(???) - TO-DO.</text>


    </stage>
    <stage id="conditionalRendering5" x="0" y="0" w="100" h="100" extends="baseStage">
        <set id="appMode" param="mode" value="interact" scope="app" />
        <param name="next" value="conditionalRendering6" />
        <param name="previous" value="conditionalRendering4" />

        <image asset="bg" x="0" y="0" w="100" h="100"/>
        <param name="heading" model="testData.ts_cond_render_stage5"/>

        <text fontsize="40" color="yellow" x="22" y="43" w="22" h="12"> Learn mode
            <event type="click">
